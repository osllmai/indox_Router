# IndoxRouter Environment Variables

# Database Configuration
# Uncomment and set if not using the default configuration in config.json
# DATABASE_URL=postgresql://indoxuser:indoxpassword@localhost:5432/indoxrouter

# Redis Configuration (optional)
# REDIS_URL=redis://localhost:6379/0

# JWT Configuration
JWT_SECRET=your_secure_jwt_secret_key
JWT_ALGORITHM=HS256
JWT_EXPIRATION=86400  # 24 hours in seconds

# Admin User Configuration
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=admin

# Server Configuration
# API_HOST=0.0.0.0
# API_PORT=8000
# DASHBOARD_HOST=0.0.0.0
# DASHBOARD_PORT=7860
# DASHBOARD_SHARE=false  # Set to true to create a public link

# Logging Configuration
# LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL

# Provider API Keys
# Uncomment and add your API keys for the providers you want to use

# OpenAI
# OPENAI_API_KEY=your_openai_api_key
# OPENAI_ORG_ID=your_openai_org_id  # Optional

# Anthropic
# ANTHROPIC_API_KEY=your_anthropic_api_key

# Mistral
# MISTRAL_API_KEY=your_mistral_api_key

# Cohere
# COHERE_API_KEY=your_cohere_api_key

# Google
# GOOGLE_API_KEY=your_google_api_key

# Meta
# META_API_KEY=your_meta_api_key

# AI21
# AI21_API_KEY=your_ai21_api_key

# Llama
# LLAMA_API_KEY=your_llama_api_key

# NVIDIA
# NVIDIA_API_KEY=your_nvidia_api_key

# Deepseek
# DEEPSEEK_API_KEY=your_deepseek_api_key

# Databricks
# DATABRICKS_API_KEY=your_databricks_api_key 