# API settings
DEBUG=false

# Server settings
HOST=0.0.0.0
PORT=8000

# Security settings
SECRET_KEY=your-secure-random-secret-key
ACCESS_TOKEN_EXPIRE_MINUTES=30

# CORS settings
CORS_ORIGINS=["https://yourdomain.com", "http://91.107.253.133"]

# External website database settings
DATABASE_URL=postgresql://dbuser:securepassword@your-website-db-host:5432/your_website_db
DB_MIN_CONNECTIONS=5
DB_MAX_CONNECTIONS=20

# Provider API keys
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key
COHERE_API_KEY=your-cohere-api-key
GOOGLE_API_KEY=your-google-api-key
MISTRAL_API_KEY=your-mistral-api-key

# Default provider and model
DEFAULT_PROVIDER=openai
DEFAULT_CHAT_MODEL=gpt-4o
DEFAULT_COMPLETION_MODEL=gpt-4o
DEFAULT_EMBEDDING_MODEL=text-embedding-3-large

# Rate limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_PERIOD_SECONDS=60 
